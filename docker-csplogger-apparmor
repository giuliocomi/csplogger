
#include <tunables/global>


profile docker-csplogger-apparmor flags=(attach_disconnected,mediate_deleted) {
  #include <abstractions/base>



  network inet tcp,

  deny network raw,

  deny network packet,

  file,
  umount,

  deny /bin/** wl,
  deny /boot/** wl,
  deny /dev/** wl,
  deny /etc/** wl,
  deny /lib/** wl,
  deny /lib64/** wl,
  deny /media/** wl,
  deny /mnt/** wl,
  deny /opt/** wl,
  deny /proc/** wl,
  deny /root/** wl,
  deny /sbin/** wl,
  deny /srv/** wl,
  deny /tmp/** wl,
  deny /sys/** wl,
  deny /usr/** wl,

  audit /** w,

  /home/csplogger-agent/csplogger/databases w,

  /bin/id ix,
  /bin/sh ix,

  deny /sbin/* mrwklx,
  deny /bin/arch mrwklx,
  deny /bin/ash mrwklx,
  deny /bin/base64 mrwklx,
  deny /bin/bbconfig mrwklx,
  deny /bin/busybox mrwklx,
  deny /bin/cat mrwklx,
  deny /bin/chgrp mrwklx,
  deny /bin/chmod mrwklx,
  deny /bin/chown mrwklx,
  deny /bin/conspy mrwklx,
  deny /bin/cp mrwklx,
  deny /bin/date mrwklx,
  deny /bin/dd mrwklx,
  deny /bin/df mrwklx,
  deny /bin/dmesg mrwklx,
  deny /bin/dnsdomainname mrwklx,
  deny /bin/dumpkmap mrwklx,
  deny /bin/echo mrwklx,
  deny /bin/ed mrwklx,
  deny /bin/egrep mrwklx,
  deny /bin/false mrwklx,
  deny /bin/fatattr mrwklx,
  deny /bin/fdflush mrwklx,
  deny /bin/fgrep mrwklx,
  deny /bin/fsync mrwklx,
  deny /bin/getopt mrwklx,
  deny /bin/grep mrwklx,
  deny /bin/gunzip mrwklx,
  deny /bin/gzip mrwklx,
  deny /bin/hostname mrwklx,
  deny /bin/ionice mrwklx,
  deny /bin/iostat mrwklx,
  deny /bin/ipcalc mrwklx,
  deny /bin/kbd_mode mrwklx,
  deny /bin/kill mrwklx,
  deny /bin/link mrwklx,
  deny /bin/linux32 mrwklx,
  deny /bin/linux64 mrwklx,
  deny /bin/ln mrwklx,
  deny /bin/login mrwklx,
  deny /bin/ls mrwklx,
  deny /bin/lzop mrwklx,
  deny /bin/makemime mrwklx,
  deny /bin/mkdir mrwklx,
  deny /bin/mknod mrwklx,
  deny /bin/mktemp mrwklx,
  deny /bin/more mrwklx,
  deny /bin/mount mrwklx,
  deny /bin/mountpoint mrwklx,
  deny /bin/mpstat mrwklx,
  deny /bin/mv mrwklx,
  deny /bin/netstat mrwklx,
  deny /bin/nice mrwklx,
  deny /bin/pidof mrwklx,
  deny /bin/ping mrwklx,
  deny /bin/ping6 mrwklx,
  deny /bin/pipe_progress mrwklx,
  deny /bin/printenv mrwklx,
  deny /bin/ps mrwklx,
  deny /bin/pwd mrwklx,
  deny /bin/reformime mrwklx,
  deny /bin/rev mrwklx,
  deny /bin/rm mrwklx,
  deny /bin/rmdir mrwklx,
  deny /bin/run-parts mrwklx,
  deny /bin/sed mrwklx,
  deny /bin/setpriv mrwklx,
  deny /bin/setserial mrwklx,
  deny /bin/sleep mrwklx,
  deny /bin/stat mrwklx,
  deny /bin/stty mrwklx,
  deny /bin/su mrwklx,
  deny /bin/sync mrwklx,
  deny /bin/tar mrwklx,
  deny /bin/touch mrwklx,
  deny /bin/true mrwklx,
  deny /bin/umount mrwklx,
  deny /bin/uname mrwklx,
  deny /bin/usleep mrwklx,
  deny /bin/watch mrwklx,
  deny /bin/zcat mrwklx,
  deny /usr/bin/* mrwklx,
  deny /usr/sbin/* mrwklx,



  deny @{PROC}/* w,   # deny write for all files directly in /proc (not in a subdir)
  deny @{PROC}/{[^1-9],[^1-9][^0-9],[^1-9s][^0-9y][^0-9s],[^1-9][^0-9][^0-9][^0-9]*}/** w,
  deny @{PROC}/sys/[^k]** w,  # deny /proc/sys except /proc/sys/k* (effectively /proc/sys/kernel)
  deny @{PROC}/sys/kernel/{?,??,[^s][^h][^m]**} w,  # deny everything except shm* in /proc/sys/kernel/
  deny @{PROC}/sysrq-trigger rwklx,
  deny @{PROC}/mem rwklx,
  deny @{PROC}/kmem rwklx,
  deny @{PROC}/kcore rwklx,
  deny mount,
  deny /sys/[^f]*/** wklx,
  deny /sys/f[^s]*/** wklx,
  deny /sys/fs/[^c]*/** wklx,
  deny /sys/fs/c[^g]*/** wklx,
  deny /sys/fs/cg[^r]*/** wklx,
  deny /sys/firmware/efi/efivars/** rwklx,
  deny /sys/kernel/security/** rwklx,
}
